{
  "name": "fonts-cjk",
  "version": "2.3.1",
  "private": true,
  "description": "Delivers better CJK fonts to Atom editor.",
  "keywords": [
    "fonts",
    "cjk",
    "chinese",
    "japanese",
    "korean",
    "monospace",
    "typography"
  ],
  "homepage": "https://atom.io/packages/fonts-cjk",
  "bugs": {
    "url": "https://github.com/jmlntw/atom-fonts-cjk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmlntw/atom-fonts-cjk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jimmy Lin",
    "email": "jmlntw@gmail.com"
  },
  "main": "./lib/fonts-cjk",
  "scripts": {
    "lint": "standard --verbose",
    "test": "npm run lint && atom --test spec"
  },
  "devDependencies": {
    "standard": "^16.0.0"
  },
  "engines": {
    "atom": ">=1.13.0 <2.0.0"
  },
  "configSchema": {
    "editorFont": {
      "type": "string",
      "title": "Editor Font",
      "description": "Use one of the fonts available in this package.",
      "order": 1,
      "default": "(Default)",
      "enum": [
        "(Default)",
        "M+ 1m",
        "M+ 2m",
        "Ricty Diminished",
        "Sarasa Mono HC",
        "Sarasa Mono J",
        "Sarasa Mono K",
        "Sarasa Mono SC",
        "Sarasa Mono TC"
      ]
    },
    "markdownPreviewFont": {
      "type": "string",
      "title": "Markdown Preview Font",
      "description": "Use one of the fonts available in this package.",
      "order": 2,
      "default": "(Default)",
      "enum": [
        "(Default)",
        "M+ 1p",
        "M+ 2p",
        "M+ 1m",
        "M+ 2m",
        "Noto Sans HK",
        "Noto Sans JP",
        "Noto Sans KR",
        "Noto Sans SC",
        "Noto Sans TC",
        "Ricty Diminished",
        "Sarasa Mono HC",
        "Sarasa Mono J",
        "Sarasa Mono K",
        "Sarasa Mono SC",
        "Sarasa Mono TC"
      ]
    },
    "workspaceFont": {
      "type": "string",
      "title": "Workspace Font",
      "description": "Use one of the fonts available in this package.",
      "order": 3,
      "default": "(Default)",
      "enum": [
        "(Default)",
        "M+ 1p",
        "M+ 2p",
        "M+ 1m",
        "M+ 2m",
        "Noto Sans HK",
        "Noto Sans JP",
        "Noto Sans KR",
        "Noto Sans SC",
        "Noto Sans TC",
        "Ricty Diminished",
        "Sarasa Mono HC",
        "Sarasa Mono J",
        "Sarasa Mono K",
        "Sarasa Mono SC",
        "Sarasa Mono TC"
      ]
    },
    "secondaryFont": {
      "type": "string",
      "title": "Secondary Font",
      "description": "Additional fonts for glyphs missing in the main font.",
      "order": 4,
      "default": ""
    }
  },
  "standard": {
    "env": [
      "browser",
      "node",
      "atomtest",
      "jasmine"
    ],
    "globals": [
      "atom"
    ]
  }
}
